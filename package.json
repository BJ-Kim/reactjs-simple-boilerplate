{
  "name": "reactjs-simple-boilerplate",
  "version": "1.0.0",
  "description": "Simple Boilerplate Project For ReactJS",
  "main": "src/server/index.js",
  "scripts": {
    "test": "jest",
    "dev": "node webpack-dev-server.js",
    "start": "node src/server/index.js | npm run dev",
    "build": "npm run webpack",
    "webpack": "NODE_ENV=production webpack --display-modules -p --config config/webpack.prod.js",
    "webpack-watch": "webpack --config config/webpack.js --watch --colors"
  },
  "author": "Eun Woo Song",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.0.14",
    "babel-core": "^6.0.14",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "css-loader": "^0.23.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.5",
    "jest-cli": "^0.8.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "react-addons-test-utils": "^0.14.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "body-parser": "^1.14.1",
    "classnames": "^2.2.1",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "debug": "^2.2.0",
    "events": "^1.1.0",
    "express": "^4.13.3",
    "flux": "^2.1.1",
    "history": "^1.13.1",
    "jquery": "^2.1.4",
    "keymirror": "^0.1.1",
    "merge": "^1.2.0",
    "morgan": "^1.6.1",
    "object-assign": "^4.0.1",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-router": "^1.0.0",
    "slugid": "^1.1.0"
  }
}
